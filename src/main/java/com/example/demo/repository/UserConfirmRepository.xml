<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.repository.UserConfirmRepository">
    <select id="selectConfirm" resultType="java.lang.String">
        SELECT user_name FROM m_user WHERE user_id = #{userId} AND password = #{password} 
    </select>
</mapper>